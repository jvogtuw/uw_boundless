{
  "name": "drupal/uw_boundless",
  "description": "UW Boundless Drupal theme running on Bootstrap 3.",
  "type": "drupal-theme",
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/jvogtuw/uw_boundless/issues",
    "source": "https://github.com/jvogtuw/uw_boundless"
  },
  "minimum-stability": "dev",
  "repositories": {
    "drupal_org": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1",
    "drupal/bootstrap": "^3.3"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true
    }
  },
  "require-dev": {
    "composer/installers": "^2",
    "cweagans/composer-patches": "~1.0",
    "drupal/core-recommended": "^10"
  }
}
